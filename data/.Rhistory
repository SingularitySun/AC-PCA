calAv <- function(v, args){
X <- args$X
K <- args$K
lambda <- args$lambda
return( crossprod(X, (diag(dim(K)[1])-lambda*K)%*%(X%*%matrix(v, ncol=1))) )
#return( crossprod(X, (diag(dim(K)[1])-lambda*K)%*%X%*%matrix(v, ncol=1)) )
}
